AI_BASE_URL=                          # Custom endpoint URL (required for ollama: http://localhost:11434)
AI_CONFIDENCE_THRESHOLD=70
AI_TIMEOUT_MS=2000

# Legacy (still supported, AI_API_KEY takes precedence)
ANTHROPIC_API_KEY=
GEMINI_API_KEY=
GROQ_API_KEY=

